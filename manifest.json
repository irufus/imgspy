{
	"manifest_version": 2,
	"name": "Img Spy"
	"description": "This extension shows basic info for images that use an <img> tag"
	"version": "1.0",
	
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
		"default_title": "Click pls"
	},
	"permissions": [
	"activeTab",
	"https://ajax.googleapis.com"]
}